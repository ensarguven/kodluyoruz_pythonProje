def flatten(x):
    for x in y:
        if isinstance(x, list):
            yield from flatten(x)
        else:
            yield x
            
x = [1,['a','a',['cat'],2],[[[3]],'dog'],4,5]
a = list(flatten(x))
print(a)
            
 
